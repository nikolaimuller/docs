{
  "name": "@mullerstd/docs",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "example:storybook": "start-storybook -p 6006 -c examples/with-storybook/.storybook",
    "build": "yarn clean && rollup -c && yarn tailwindcss -i ./src/styles/main.css -o ./dist/styles.css",
    "clean": "rimraf dist",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "prettier": "prettier --write .",
    "eslint": "eslint .",
    "stylelint": "stylelint --allow-empty-input --ignore-disables --config ./stylelint.config.js --max-warnings 25 --color '**/*.css'",
    "pre-commit": "yarn typecheck && yarn eslint && yarn stylelint && yarn prettier"
  },
  "devDependencies": {
    "@mullerstd/eslint-config": "^3.0.1",
    "@mullerstd/prettier-config": "^1.0.2",
    "@mullerstd/stylelint-config": "^2.0.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.1.41",
    "@storybook/react": "^6.5.16",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "postcss": "^8.4.22",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "rimraf": "^5.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "storybook-dark-mode": "^1.1.2",
    "stylelint": "^14.16.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^2.9.15"
  },
  "dependencies": {
    "@babel/parser": "^7.21.4",
    "classnames": "^2.3.2",
    "prettier": "^2.8.7",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^3.2.0"
  }
